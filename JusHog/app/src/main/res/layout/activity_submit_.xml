<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_submit_"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#E5FFFC"
    tools:context="com.rm.dell.jushog.Submit_Activity">

    <TextView
        android:layout_width="40dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView6"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp"
        android:id="@+id/ssex"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="cursive"
        android:typeface="normal"
        android:textSize="24sp"
        android:textColor="#000000"
        android:textStyle="normal|bold" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/sage"
        android:layout_alignBaseline="@+id/textView11"
        android:layout_alignBottom="@+id/textView11"
        android:typeface="normal"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:textColor="#000000"
        android:textSize="24sp"
        android:layout_toRightOf="@+id/textView9"
        android:layout_toEndOf="@+id/textView9" />

    <TextView
        android:text="Your Number :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="43dp"
        android:id="@+id/textView13"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="serif-monospace"
        android:typeface="normal"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:textColor="#000000"
        android:layout_below="@+id/sage"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11" />

    <TextView
        android:text="Hello"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:id="@+id/textView6"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="serif-monospace"
        android:textSize="30sp"
        android:textColor="#000000"
        android:textStyle="normal|bold"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Your Age Is : "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="59dp"
        android:id="@+id/textView11"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="serif-monospace"
        android:textSize="30sp"
        android:typeface="normal"
        android:textStyle="normal|bold"
        android:textColor="#000000"
        android:layout_below="@+id/textView9"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Your Details"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:fontFamily="casual"
        android:typeface="serif"
        android:textSize="56sp"
        android:layout_marginTop="15dp"
        android:textStyle="normal|bold"
        android:textColor="#D4AF37"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Rm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/textView2"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:fontFamily="casual"
        android:textSize="18sp"
        android:textColor="#000000"
        android:typeface="normal" />

    <TextView
        android:text="Your Email Is :"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:id="@+id/textView9"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="serif-monospace"
        android:typeface="normal"
        android:textSize="30sp"
        android:textStyle="normal|bold"
        android:textColor="#000000"
        android:layout_below="@+id/ssex"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:id="@+id/semail"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="cursive"
        android:typeface="normal"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:textColor="#000000"
        android:layout_above="@+id/sage"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/sphone"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="cursive"
        android:typeface="normal"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        android:textColor="#000000"
        android:layout_above="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="19dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/sname"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:fontFamily="cursive"
        android:typeface="normal"
        android:textStyle="normal|bold"
        android:textSize="24sp"
        android:textColor="#000000"
        android:layout_above="@+id/textView9"
        android:layout_toRightOf="@+id/ssex"
        android:layout_toEndOf="@+id/ssex" />

</RelativeLayout>
